<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<HBox BorderPane.alignment="CENTER"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.controller.ControlPaneController">

   <children>
      <Button fx:id="prevButton" mnemonicParsing="false" text="Preview" />
      <ToggleButton fx:id="playButton" mnemonicParsing="false" text="Play/Pause" />
      <Button fx:id="nextButton" mnemonicParsing="false" text="Next" />
      <Slider fx:id="volumeSlider" />
      <Slider fx:id="songSlider" />
   </children>

</HBox>
